{
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">1%",
      "ie >= 11",
      "edge >= 14",
      "not dead",
      "not op_mini all"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@formatjs/intl-pluralrules": "~3.4.0",
    "@formatjs/intl-relativetimeformat": "~7.2.0",
    "@loft/copan-components": "^1.8.3",
    "@reduxjs/toolkit": "^1.4.0",
    "@zeit/next-css": "~1.0.1",
    "axe-core": "^4.0.2",
    "axios": "~0.19.2",
    "clsx": "~1.1.1",
    "cookie-parser": "~1.4.5",
    "cross-env": "~7.0.2",
    "cypress-axe": "^0.10.0",
    "es-check": "~5.1.0",
    "express": "~4.17.1",
    "formik": "~2.1.5",
    "fs": "0.0.1-security",
    "full-icu": "~1.3.1",
    "gray-matter": "^4.0.3",
    "intl": "~1.2.5",
    "newrelic": "~6.12.0",
    "next": "~9.5.2",
    "next-compose-plugins": "~2.2.0",
    "next-optimized-images": "~2.6.2",
    "next-redux-wrapper": "~6.0.0",
    "next-transpile-modules": "~4.1.0",
    "nprogress": "~0.2.0",
    "path": "^0.12.7",
    "react": "~16.13.0",
    "react-dom": "~16.13.0",
    "react-hook-form": "~5.7.0",
    "react-intl": "~5.6.5",
    "react-redux": "~7.2.1",
    "redux": "~4.0.5",
    "redux-devtools-extension": "~2.13.8",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "rimraf": "~3.0.2",
    "sass": "^1.32.13"
  },
  "devDependencies": {
    "@commitlint/cli": "~9.1.2",
    "@commitlint/config-conventional": "~9.1.2",
    "@loft/eslint-plugin": "^1.1.0",
    "@next/bundle-analyzer": "~9.5.2",
    "@testing-library/jest-dom": "~5.11.3",
    "@testing-library/react": "~10.4.8",
    "@types/cookie-parser": "~1.4.2",
    "@types/express": "~4.17.3",
    "@types/intl": "~1.2.0",
    "@types/jest": "~26.0.10",
    "@types/newrelic": "~6.4.0",
    "@types/next": "~9.0.0",
    "@types/next-redux-wrapper": "~3.0.0",
    "@types/node": "~14.6.0",
    "@types/nprogress": "~0.2.0",
    "@types/react": "~16.9.46",
    "@types/react-dom": "~16.9.5",
    "@types/react-redux": "~7.1.9",
    "babel-plugin-react-intl": "~8.1.3",
    "git-cz": "~4.7.0",
    "husky": "~4.2.3",
    "imagemin-mozjpeg": "~9.0.0",
    "imagemin-pngquant": "~9.0.0",
    "jest": "~26.4.0",
    "lint-staged": "~10.2.6",
    "lqip-loader": "~2.2.0",
    "nodemon": "~2.0.2",
    "plop": "~2.7.4",
    "responsive-loader": "~2.0.2",
    "svg-sprite-loader": "~5.0.0",
    "ts-node": "~8.10.1",
    "typescript": "~3.9.7",
    "url-loader": "~4.1.0",
    "webp-loader": "~0.6.0"
  },
  "engineStrict": true,
  "engines": {
    "node": "14.x.x"
  },
  "husky": {
    "hooks": {
      "commit-msg": "scripts/branchlint.sh && commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "scripts/ts-check.sh && scripts/copanlint.sh && lint-staged",
      "pre-push": "npm run test && npm run analyze"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet"
    ],
    ".eslintrc.js": [
      "npx eslint src --quiet"
    ]
  },
  "name": "test-research",
  "peerDependencies": {
    "@loft/copan-tokens": "*",
    "@material-ui/core": "*",
    "@material-ui/icons": "*",
    "@material-ui/lab": "*",
    "@material-ui/styles": "*"
  },
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "npm run build:next && npm run build:server",
    "build:next": "rimraf .next && NODE_ENV=production next build && npm run lint:es5",
    "build:server": "rimraf dist && tsc --project tsconfig.server.json",
    "commit": "git-cz",
    "dev": "NEW_RELIC_ENABLED=false nodemon",
    "e2e:ci": "docker-compose up --exit-code-from cypress",
    "e2e:dev": "npx cypress@5.6.0 open -C cypress/cypress.json",
    "lint": "eslint src --ext ts,tsx,js --quiet",
    "lint:es5": "scripts/check-es5.sh",
    "start": "NODE_ENV=production node dist/index.js",
    "test": "jest --silent",
    "test:coverage": "npm run test -- --coverage --collectCoverageFrom=src/**/*.{js,jsx,ts,tsx}"
  },
  "version": "1.0.0"
}
